44. function findMaximum(arr) {
    if (arr.length === 0) {
        return undefined; 
    }

    let max = arr[0]; 

    for (let i = 1; i < arr.length; i++) {
        if (arr[i] > max) {
            max = arr[i]; 
        }
    }

    return max; 
}

let arr=[4, 7, 8, 97, 56]
console.log(findMaximum(arr));

output: 97

